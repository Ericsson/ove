#!/usr/bin/env bash

function test_buildme() {
	if [ "${nbr_of_projs:?}" -eq 0 ]; then
		return 0
	fi

	ove buildme
	assertEquals "0" "$?"

	ove buildme p1 l1
	assertEquals "0" "$?"
}

function test_buildme_parallel() {
	if [ "${nbr_of_projs:?}" -eq 0 ]; then
		return 0
	fi

	ove buildme-parallel
	assertEquals "0" "$?"

	ove buildme-parallel p1 l1
	assertEquals "0" "$?"
}

# shellcheck disable=SC1090
source "${OVE_DIR}/tests/common.sh"
