#!/usr/bin/env bash

function testStatus() {
	ove status | grep -q "git-.*${OVE_BRANCH_NAME_DEFAULT:?}.*origin"
	ret=$?
	assertEquals "0" $ret
}

function testNumberOfRepos() {
	num=$(ove status | wc -l)
	assertEquals "${nbr_of_repos:?}" "$num"
}

# shellcheck disable=SC1090
source "${OVE_DIR}/tests/common.sh"
