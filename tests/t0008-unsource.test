#!/usr/bin/env bash

function test_unsource() {
	ove unsource
	set | grep -i ^ove
	assertEquals "1" "$?"

	set > before
	# shellcheck disable=SC1091
	source ove
	set > after
	ove unsource
	set > unsource

	sed -i \
		-e '/^BASH.*/d' \
		-e '/^PIPESTATUS=.*/d' \
		-e '/^OLDPWD=.*/d' \
		-e '/^_=.*/d' \
		-e '/TIMEFORMAT=.*/d' \
		-e '/EPOCHREALTIME=.*/d' \
		before unsource
	assertEquals "0" "$(diff -U0 before unsource | wc -l)"
	rm after before unsource
	assertEquals "0" "$?"
}

# shellcheck disable=SC1090
source "${OVE_DIR}/tests/common.sh"
